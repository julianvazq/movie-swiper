urn errors;\r\n    }\r\n\r\n    const onSubmit = (e: React.FormEvent<HTMLFormElement>) => {\r\n        e.preventDefault();\r\n        setSubmitted(true);\r\n\r\n        if (!isFormValid) return;\r\n\r\n        const newUser = {\r\n            ...user,\r\n            name: inputs.name,\r\n            color: randomColor({\r\n                luminosity: 'light',\r\n                hue: 'blue',\r\n            }),\r\n        };\r\n        setUser(newUser);\r\n        history.push(`/selection/${roomId}`);\r\n    };\r\n\r\n    return (\r\n        <StyledForm onSubmit={onSubmit}>\r\n            <InputGroup>\r\n                <label>Your name</label>\r\n                <input type=\"text\" name=\"name\" onChange={onChange} onBlur={onBlur} />\r\n                {(touched.name || submitted) && errors.name && <ErrorMessage>{errors.name}</ErrorMessage>}\r\n            </InputGroup>\r\n            <FormButton>Get Started</FormButton>\r\n        </StyledForm>\r\n    );\r\n};\r\n\r\nexport default JoinForm;\r\n","C:\\Users\\Julian\\Documents\\Julian\\J\\Programming\\Web Dev\\Projects\\React\\movie-swiper\\client\\src\\types\\actions.ts",[],"C:\\Users\\Julian\\Documents\\Julian\\J\\Programming\\Web Dev\\Projects\\React\\movie-swiper\\client\\src\\types\\room.ts",[],"C:\\Users\\Julian\\Documents\\Julian\\J\\Programming\\Web Dev\\Projects\\React\\movie-swiper\\client\\src\\pages\\expired\\Expired\\index.tsx",[],"C:\\Users\\Julian\\Documents\\Julian\\J\\Programming\\Web Dev\\Projects\\React\\movie-swiper\\client\\src\\pages\\expired\\Expired\\style.ts",[],"C:\\Users\\Julian\\Documents\\Julian\\J\\Programming\\Web Dev\\Projects\\React\\movie-swiper\\client\\src\\sockets\\listeners.ts",[],"C:\\Users\\Julian\\Documents\\Julian\\J\\Programming\\Web Dev\\Projects\\React\\movie-swiper\\client\\src\\pages\\swiper\\Swiper\\index.tsx",[],"C:\\Users\\Julian\\Documents\\Julian\\J\\Programming\\Web Dev\\Projects\\React\\movie-swiper\\client\\src\\pages\\selection\\MovieSelection\\index.tsx",[],"C